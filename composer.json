{
    "name": "laraflux/lens",
    "description": "Laravel Activity Log",
    "license": "MIT",
    "authors": [
        {
            "name": "Laraflux",
            "email": "trev@notano.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Laraflux\\Lens\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraflux\\Lens\\Tests\\": "tests",
            "Laraflux\\Lens\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraflux\\Lens\\Providers\\LensServiceProvider",
                "Laraflux\\Lens\\Providers\\EventServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.5"
    }
}
